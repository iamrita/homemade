<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/back_button_confirm_local"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_margin="5dp"
            app:srcCompat="@drawable/back" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="300dp"
            android:layout_height="300dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/noun_checkmark_569906"/>


        <TextView
            android:id="@+id/textView72"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Your scheduling request has been sent to Amrita."
            android:textAlignment="center"
            android:textColor="@android:color/black"
            android:textSize="32sp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="346dp"
            android:layout_gravity="center" />

        <TextView
            android:id="@+id/textView73"
            android:layout_width="223dp"
            android:layout_height="64dp"
            android:text="The status of the request will be refleted in Meals."
            android:textAlignment="center"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="81dp"
            tools:layout_editor_absoluteY="431dp"
            android:layout_gravity="center"/>

    </LinearLayout>



</android.support.constraint.ConstraintLayout>